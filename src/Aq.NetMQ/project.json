    "netstandard1.3": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [
          "NETCORE"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6",
        "System.Reflection.Emit": "4.*"
      }
    }
  }
